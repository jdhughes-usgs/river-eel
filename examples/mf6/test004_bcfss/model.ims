begin options
  PRINT_OPTION ALL
  CSV_OUTPUT FILEOUT bcf2ss.ims.csv
	COMPLEXITY SIMPLE
end options

begin nonlinear
	outer_hclose 1.e-6
	outer_maximum 500 
	under_relaxation none
end nonlinear

begin linear
	inner_hclose 1.0e-6
	inner_rclose 0.001
	#L2NORM_RCLOSE
	inner_maximum 100
	linear_acceleration cg
	scaling_method none
	REORDERING_METHOD none
	relaxation_factor 0.97
end linear
